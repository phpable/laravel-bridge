	"description": "Bridge classes to use phpABLE components inside of a Laravel application",
	"license": "MIT",
	"authors": [
		{
			"name": "hacpaka",
			"email": "hacpaka@eggbe.com"
		}
	],
	"require": {
		"php": ">=7.2.0",
